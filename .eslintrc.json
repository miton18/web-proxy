{
    "extends": "google",
    "installedESLint": true,
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },

    "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "max-len": ["error", 100],
        "comma-dangle": ["error", "never"],
        "no-multi-spaces": 0
    },
    "parserOptions": { 
        "ecmaVersion": 6, 
        "sourceType": "module", 
        "ecmaFeatures": { 
            "impliedStrict": true, 
            "modules": false 
        } 
    }
}
